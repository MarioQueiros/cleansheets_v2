<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="csheets.io.XMLVersionControl" table="VERSIONCONTROL">
        <composite-id name="m_key">
            <key-property name="m_filename" column="FILENAME" type="java.lang.String" length="100"/>
            <key-property name="m_timestamp" column="TIMESTAMPFILE" type="java.sql.Timestamp" />
        </composite-id>
        <property name="m_id" type="int">
            <column name="ID" />
        </property>
        <property name="m_blob" type="java.sql.Blob">
            <column name="BLOBFILE" />
        </property>
    </class>
</hibernate-mapping>
